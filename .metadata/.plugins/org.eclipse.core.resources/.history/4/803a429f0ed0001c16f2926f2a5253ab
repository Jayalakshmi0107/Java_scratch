package day7;

public class SearchingElementInArray {

	public static void main(String[] args) 
	{
		int a[]= {50,30,20,10,40};
		int num=200;
		for(int i=0;i<a.length;i++)
		{
			if(a[i]==num)
			{
				System.out.println("value matched " +a[i]);
			}
			else
			{
				System.out.println("Element not found ");
			}
		}

	}

}
