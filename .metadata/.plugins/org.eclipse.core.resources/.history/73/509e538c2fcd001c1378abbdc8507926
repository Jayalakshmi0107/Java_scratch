package day4;

public class IfElseEvenOrOdd {

	public static void main(String[] args) {
		int i=11;
		
			if(i%2==0)
			{
				System.out.println("Given number is even");
			}
			else
			{
				System.out.println("Given number is odd");
			}
		int num=12;
		
		String res=num%2==0? "Even" : "Odd";
		System.out.println("Number is "+res);
		
		
		
	}

}
